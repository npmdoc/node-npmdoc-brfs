{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "brfs",
    "description": "api documentation for [brfs (v1.4.3)](https://github.com/substack/brfs) [![npm package](https://img.shields.io/npm/v/npmdoc-brfs.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-brfs) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-brfs.svg)](https://travis-ci.org/npmdoc/node-npmdoc-brfs)",
    "devDependencies": {
        "brfs": "*",
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-brfs",
    "keywords": [
        "documentation",
        "brfs"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_brfs.js",
    "name": "npmdoc-brfs",
    "nameAlias": "npmdoc_brfs",
    "nameOriginal": "npmdoc-brfs",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-brfs.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.1"
}
